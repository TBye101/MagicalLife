    <EmbeddedResource Remove="Components\Item\**" />
    <EmbeddedResource Remove="Entity\Animal\**" />
    <EmbeddedResource Remove="Entity\Entity Components\**" />
    <EmbeddedResource Remove="World\Veggies\**" />
