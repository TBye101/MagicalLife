  <ItemGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|x64' ">
    <Content Include="..\Included DLL\fmod64.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
