  <ItemGroup>
    <Compile Remove="Components\Generic\**" />
    <Compile Remove="Components\Item\**" />
    <Compile Remove="Entity\Animal\**" />
    <Compile Remove="Entity\Entity Components\**" />
    <Compile Remove="World\Veggies\**" />
